start
	Declarations
		num hrsPerDay
		num hrsInWorkWeek
		num hrsInWorkYear
		num DAYS_IN_WORK_WEEK = 5
		num DAYS_IN_WORK_YEAR = 252

	output "Please enter your hours you work every work day"

	input hrsPerDay

	set hrsInWorkWeek = hrsPerDay * DAYS_IN_WORK_WEEK
	set hrsInWorkYear = hrsPerDay * DAYS_IN_WORK_YEAR

	output "You work ", hrsInWorkWeek, " hours in a work week"
	output "You work ", hrsInWorkYear, " hours in a work year"
stop